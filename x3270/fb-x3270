	:<Key>KP_8:		Key(8)\n\
	:<Key>KP_9:		Key(9)\n\
	:<Key>KP_0:		Key(0)\n\
	:<Key>KP_Decimal:	Key(.)\n\
	:<Key>F18:		insert-selection(PRIMARY)\n\
	:<Key>F19:		SysReq()\n\
	:<Key>F20:		FieldMark()\n\
	:<Key>F21:		PA(1)\n\
	:<Key>F22:		PA(2)\n\
	:<Key>F23:		Dup()\n\
	:<Key>F24:		Reset()\n\
	:<Key>F25:		EraseEOF()\n\
