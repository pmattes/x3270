# Makefile for pr3287
top = ../../..
objdir = ../obj/@host@/pr3287
this = $(top)/pr3287

export VPATH = $(this):$(top)/Common/pr3287:$(top)/Common
export TOP = $(top)
export THIS = $(this)

MAKEINC = -I$(this) -I$(top)/Common -I$(top)/Common/pr3287

all: $(objdir)
	cd $(objdir) && $(MAKE) $(MAKEINC) -f $(this)/Makefile.obj $@
install: $(objdir)
	cd $(objdir) && $(MAKE) $(MAKEINC) -f $(this)/Makefile.obj $@
install.man: $(objdir)
	cd $(objdir) && $(MAKE) $(MAKEINC) -f $(this)/Makefile.obj $@
clean: $(objdir)
	cd $(objdir) && $(MAKE) $(MAKEINC) -f $(this)/Makefile.obj $@
clobber: $(objdir)
	cd $(objdir) && $(MAKE) $(MAKEINC) -f $(this)/Makefile.obj $@

$(objdir):
	mkdir -p $(objdir)
